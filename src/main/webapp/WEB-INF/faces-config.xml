<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
	</navigation-rule>
	<!--menu-->
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>welcome</from-outcome>
			<to-view-id>/secured/welcome.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>registerUser</from-outcome>
			<to-view-id>/secured/teacher-section/admin-section/registerUser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>deleteUser</from-outcome>
			<to-view-id>/secured/teacher-section/admin-section/deleteUser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>changePassword</from-outcome>
			<to-view-id>/secured/changePassword.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>getQuestion</from-outcome>
			<to-view-id>/secured/teacher-section/getQuestion.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>getAccessTokens</from-outcome>
			<to-view-id>/secured/teacher-section/getAccessTokens.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
    <!--login page-->
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>loginSuccessful</from-outcome>
			<to-view-id>/secured/welcome.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
    <!--questionLogic page-->
	<navigation-rule>
		<display-name>questionLogic.xhtml</display-name>
		<from-view-id>/secured/questionLogic.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>quit</from-outcome>
			<to-view-id>/secured/welcome.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>quitError</from-outcome>
			<to-view-id>/secured/welcome-error.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
    <!--welcome page-->
    <navigation-rule>
        <display-name>welcome</display-name>
        <from-view-id>/secured/welcome.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>questionLogic</from-outcome>
            <to-view-id>/secured/questionLogic.xhtml</to-view-id>
			<redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>resultLogic</from-outcome>
            <to-view-id>/secured/resultLogic.xhtml</to-view-id>
        </navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
    </navigation-rule>
	<!--welcome error page-->
	<navigation-rule>
		<display-name>welcome-error</display-name>
		<from-view-id>/secured/welcome-error.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>questionLogic</from-outcome>
			<to-view-id>/secured/questionLogic.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>resultLogic</from-outcome>
			<to-view-id>/secured/resultLogic.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!--get question-->
	<navigation-rule>
		<display-name>getQuestion</display-name>
		<from-view-id>/secured/teacher-section/getQuestion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editQuestion</from-outcome>
			<to-view-id>/secured/teacher-section/editQuestion.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>addNewQuestion</from-outcome>
			<to-view-id>/secured/teacher-section/addNewQuestion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!--new question-->
	<navigation-rule>
		<display-name>addNewQuestions</display-name>
		<from-view-id>/secured/teacher-section/addNewQuestion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>allQuestions</from-outcome>
			<to-view-id>/secured/teacher-section/getQuestion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!--edit question-->
	<navigation-rule>
		<display-name>editQuestion</display-name>
		<from-view-id>/secured/teacher-section/editQuestion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>allQuestions</from-outcome>
			<to-view-id>/secured/teacher-section/getQuestion.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	<lifecycle>
	</lifecycle>
</faces-config>
